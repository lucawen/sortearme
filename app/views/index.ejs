<!DOCTYPE html>
<html ng-app="sortearme">
    <head>
        <meta charset="UTF-8">
        <link href="http://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
        <!-- build:css /css/index.min.css -->
        <link rel="stylesheet" href="vendor/Materialize/dist/css/materialize.css" media="screen,projection">
        <link rel="stylesheet" href="assets/css/main.css">
        <!-- endbuild -->
        <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    </head>
    <body ng-init="searchOwnerId='<%=userId%>'">
        <% if(!userId=='') {%>
        <div class="container" ng-cloak>
           <div class="section">
            <div class="row ">
                <div class="menuLeft z-depth-2 col l4 s12">
                    <% if(userName!='') { %>
                        <div class="section">
                            <div class="nameUser hide-on-small-only">
                                <p class="center-align"><%=userName%></p>
                            </div>
                            <div class="center-align imgUser hide-on-small-only">
                                <img src="<%=avatarUrl%>" alt="Avatar" class="circle">
                            </div>
                            <div class="center-align userName hide-on-small-only">
                                <% if(userLogged){%>
                                <h6>@<%=userLogged%></h6>
                                <%} else {%>
                                <h6></h6>
                                <%}%>
                                <% if(userProvider=='github'){%>
                                <img src="assets/img/icons/github.png" alt="auth" class="responsive-img">
                                <%} else if (userProvider=='twitter') {%>
                                <img src="assets/img/icons/twitter.png" alt="auth" class="responsive-img">
                                <%} else if (userProvider=='google') {%>
                                <img src="assets/img/icons/google.png" alt="auth" class="responsive-img">
                                <%} else if (userProvider=='facebook') {%>
                                <img src="assets/img/icons/facebook.png" alt="auth" class="responsive-img">
                                <%}%>
                            </div>
                            <div class="row hide-on-med-and-up">
                                <div class="col s6">
                                    <div class="chip">
                                        <img src="<%=avatarUrl%>" alt="Avatar" alt="Contact Person">
                                        <%=userName%>
                                    </div>
                                    <div class="nameUser center-align">
                                        <% if(userLogged){%>
                                        <h6>@<%=userLogged%></h6>
                                        <%} else {%>
                                        <h6></h6>
                                        <%}%>
                                    </div>
                                </div>
                                <div class="col s2" >
                                    <div class="providerIcon">
                                        <% if(userProvider=='github'){%>
                                        <img src="assets/img/icons/github.png" alt="auth" class="responsive-img">
                                        <%} else if (userProvider=='twitter') {%>
                                        <img src="assets/img/icons/twitter.png" alt="auth" class="responsive-img">
                                        <%} else if (userProvider=='google') {%>
                                        <img src="assets/img/icons/google.png" alt="auth" class="responsive-img">
                                        <%} else if (userProvider=='facebook') {%>
                                        <img src="assets/img/icons/facebook.png" alt="auth" class="responsive-img">
                                        <%}%>
                                    </div>
                                </div>
                                <div class="col s4">
                                    <div class="center-align" btnLists> <a href="/auth/logout" class="waves-effect waves-red btn">Sair</a> </div>
                                </div>
                            </div>
                        </div>
                        <div class="divider hide-on-small-only"></div>
                        <div class="section hide-on-small-only">
                            <div class="center-align" btnLists> <a href="/auth/logout" class="waves-effect waves-red btn">Sair</a> </div>
                        </div>
                    <% } %>
                </div>
                <div class="menuRight z-depth-2 col l8 s12">
                    <div ui-view></div>
                </div>
                </div>
            </div>
        </div>
        <% } else {%>
        <div ui-view></div>
        <% }%>



        <!-- build:js /js/index.min.js -->
        <script src="vendor/jquery/dist/jquery.js"></script>
        <script src="vendor/angular/angular.js"></script>
        <script src="vendor/angular-ui-router/release/angular-ui-router.js"></script>
        <script src="vendor/angular-resource/angular-resource.js"></script>
        <script src="vendor/Materialize/dist/js/materialize.js"></script>
        <script src="js/main.js"></script>
        <script src="js/services/listaService.js"></script>
        <script src="js/services/interceptorService.js"></script>
        <script src="js/controllers/ListasController.js"></script>
        <script src="js/controllers/ListaController.js"></script>
        <script src="js/filters/cut.js"></script>
        <script src="js/filters/split.js"></script>
        <script src="js/filters/dirPagination.js"></script>
        <!-- endbuild -->
    </body>
</html>
